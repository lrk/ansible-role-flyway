#jinja2:lstrip_blocks: True
#jinja2:trim_blocks: True
# Flyway configuration
# see https://flywaydb.org/documentation/configfiles

{% if ( flyway_url is defined ) and ( flyway_url != None) %}
  flyway.url={{ flyway_url }}
{% endif %}
{% if ( flyway_driver is defined ) and ( flyway_driver != None) %}
  flyway.driver={{ flyway_driver }}
{% endif %}
{% if ( flyway_user is defined ) and ( flyway_user != None) %}
  flyway.user={{ flyway_user }}
{% endif %}
{% if ( flyway_password is defined ) and ( flyway_password != None) %}
  flyway.password={{ flyway_password }}
{% endif %}
{% if ( flyway_connect_retries is defined ) and ( flyway_connect_retries != None) %}
  flyway.connectRetries={{ flyway_connect_retries }}
{% endif %}
{% if ( flyway_init_sql is defined ) and ( flyway_init_sql != None) %}

  flyway.initSql={{ flyway_init_sql }}
{% endif %}
{% if ( flyway_schemas is defined ) and ( flyway_schemas ) %}

  flyway.schemas={{ flyway_schemas | join(',') }}
{% endif %}
{% if ( flyway_table is defined ) and ( flyway_table != None) %}
  flyway.table={{ flyway_table }}
{% endif %}
{% if ( flyway_tablespace is defined ) and ( flyway_tablespace != None) %}
  flyway.tablespace={{ flyway_tablespace }}
{% endif %}
{% if ( flyway_locations is defined ) and ( flyway_locations ) %}

  flyway.locations={{ flyway_locations | join(',') }}
{% endif %}
{% if ( flyway_resolvers is defined ) and ( flyway_resolvers ) %}

  flyway.resolvers={{ flyway_resolvers | join(',') }}
{% endif %}
flyway.skipDefaultResolvers={{ flyway_skip_default_resolvers | default(False) | lower }}
{% if ( flyway_jar_dirs is defined ) and ( flyway_jar_dirs ) %}

  flyway.jarDirs={{ flyway_jar_dirs | join(',') }}

{% endif %}
flyway.sqlMigrationPrefix={{ flyway_sql_migration_prefix | default('V') }}
{% if (flyway_edition is defined) and (flyway_edition != None) and (flyway_edition != "community") %}
  flyway.undoSqlMigrationPrefix={{ flyway_undo_sql_migration_prefix | default('U') }}
{% endif %}
flyway.repeatableSqlMigrationPrefix={{ flyway_repeatable_sql_migration_prefix | default('R') }}
flyway.sqlMigrationSeparator={{ flyway_sql_migration_separator | default('__') }}
{% if (flyway_sql_migration_suffix is defined) and (flyway_sql_migration_suffix != None) %}
  flyway.sqlMigrationSuffix={{ flyway_sql_migration_suffix | default('.sql') }}
{% endif %}
{% if (flyway_sql_migration_suffixes is defined) and (flyway_sql_migration_suffixes) %}
  flyway.sqlMigrationSuffixes={{ flyway_sql_migration_suffix | join(',') }}
{% endif %}
{% if (flyway_edition is defined) and (flyway_edition != None) and (flyway_edition != "community") %}

  flyway.stream={{ flyway_stream | default(False) | lower }}
  flyway.batch={{ flyway_batch | default(False) | lower }}

{% endif %}
flyway.encoding={{ flyway_encoding | default('UTF-8') }}

flyway.placeholderReplacement={{ flyway_placeholder_replacement | default(True) | lower }}
{% if ( flyway_placeholders_user is defined ) and ( flyway_placeholders_user != None) %}
  flyway.placeholders.user={{ flyway_placeholders_user }}
{% endif %}
{% if ( flyway_placeholders_my_other_placeholder is defined ) and ( flyway_placeholders_my_other_placeholder != None) %}
  flyway.placeholders.my_other_placeholder={{ flyway_placeholders_my_other_placeholder }}
{% endif %}
flyway.placeholderPrefix={{ flyway_placeholder_prefix | default('${') }}
flyway.placeholderSuffix={{ flyway_placeholder_suffix | default('}') }}
{% if ( flyway_target is defined ) and ( flyway_target != None) %}

  flyway.target={{ flyway_target }}

{% endif %}
flyway.validateOnMigrate={{ flyway_validate_on_migrate | default(True) |Â lower }}
flyway.cleanOnValidationError={{ flyway_clean_on_validation_error | default(False) | lower }}
flyway.cleanDisabled={{ flyway_clean_disabled | default(False) | lower }}

flyway.baselineVersion={{ flyway_baseline_version | default(1) }}
{% if ( flyway_baseline_description is defined ) and ( flyway_baseline_description != None) %}
  flyway.baselineDescription={{ flyway_baseline_description }}
{% endif %}
flyway.baselineOnMigrate={{ flyway_baseline_on_migrate | default(False) | lower }}

flyway.outOfOrder={{ flyway_out_of_order | default(False) | lower }}
{% if (flyway_edition is defined) and (flyway_edition != None) and (flyway_edition != "community") %}

  flyway.outputQueryResults={{ flyway_output_query_results | default(true) }}*
{% endif %}
{%if ( flyway_callbacks is defined ) and ( flyway_callbacks ) %}

  flyway.callbacks={{ flyway_callbacks | join(',') }}
{% endif %}
flyway.skipDefaultCallbacks={{ flyway_skip_default_callbacks | default(False) | lower }}

flyway.ignoreMissingMigrations={{ flyway_ignore_missing_migrations | default(False) | lower }}
flyway.ignoreIgnoredMigrations={{ flyway_ignore_ignored_migrations | default(False) | lower }}
flyway.ignorePendingMigrations={{ flyway_ignore_pending_migrations | default(False) | lower }}
flyway.ignoreFutureMigrations={{ flyway_ignore_future_migrations | default(False) | lower }}

flyway.mixed={{ flyway_mixed | default(False) | lower }}

flyway.group={{ flyway_group | default(False) | lower }}
{% if ( flyway_installed_by is defined ) and ( flyway_installed_by != None) %}

  flyway.installedBy={{ flyway_installed_by }}
{% endif %}
{% if (flyway_edition is defined) and (flyway_edition != None) and (flyway_edition != "community") %}
  {% if (flyway_error_overrides is defined) and (flyway_error_overrides != None) %}

    flyway.errorOverrides={{ flyway_error_overrides }}
  {% endif %}
  {% if (flyway_dry_run_output is defined) and (flyway_dry_run_output != None) %}

    flyway.dryRunOutput={{ flyway_dry_run_output }}
  {% endif %}
  flyway.oracle.sqlplus={{ flyway_oracle_sqlplus | default(False) | lower }}
  flyway.oracle.sqlplusWarn={{ flyway_oracle_sqlplus_warn | default(False) | lower }}
{% endif %}
{% if ( flyway_license_key is defined ) and ( flyway_license_key != None) %}

  flyway.licenseKey= {{ flyway_license_key }}
{% endif %}
